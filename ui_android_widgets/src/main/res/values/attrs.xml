<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="IconTogglableButton">
        <attr name="firstIconShown" format="boolean" />
        <attr name="jht_first_state" format="boolean" />
        <attr name="jht_second_state" format="boolean" />
        <attr name="jht_third_state" format="boolean" />
    </declare-styleable>

	<declare-styleable name="JHTAutofitTextView">
	    <attr name="JHTAutofitTextView_minTextSize" format="dimension" />
	    <attr name="JHTAutofitTextView_sizeToFit" format="boolean" />
	    <attr name="JHTAutofitTextView_preferSingleLine" format="boolean" />
	</declare-styleable>


    <declare-styleable name="HelpButton">
        <attr name="title" format="string" />
        <attr name="message" format="string" />
    </declare-styleable>

    <declare-styleable name="PageIndicator">
        <attr name="horizontal" format="boolean" />
        <attr name="view_pager" format="reference" />
    </declare-styleable>

    <declare-styleable name="Keypad">
        <attr name="keypad_number_key_background" format="reference" />
        <attr name="keypad_value_background" format="color" />
        <attr name="keypad_back_key_background" format="reference" />
        <attr name="keypad_confirm_key_background" format="reference" />
        <attr name="keypad_confirm_key_src" format="reference" />
        <attr name="keypad_background" format="color" />
        <attr name="keypad_auto_hide" format="integer" />
    </declare-styleable>




    <declare-styleable name="JHTGeneralWidget">
        <attr name="units" format="enum">
            <enum name="none" value="0" />
            <enum name="distance" value="1" />
            <enum name="distance_small" value="2" />
            <enum name="speed" value="3" />
            <enum name="heart_rate" value="4" />
            <enum name="time_seconds" value="5" />
            <enum name="time_minutes" value="6" />
            <enum name="time_hours" value="7" />
            <enum name="percent" value="8" />
            <enum name="calories" value="9" />
            <enum name="weight" value="10" />
            <enum name="floors" value="11" />
            <enum name="spm" value="12" />
            <enum name="rpm" value="13" />
            <enum name="height" value="14" />
            <enum name="time_years" value="15" />
            <enum name="adc" value="16" />
        </attr>
        <attr name="decimal_format" format="string" />
        <attr name="rounding_mode" format="enum">
            <enum name="ceiling" value="0" />
            <enum name="floor" value="1" />
            <enum name="half_up" value="2" />
            <enum name="half_down" value="3" />
        </attr>
        <attr name="label" format="string" />
        <attr name="read_only" format="boolean" />
        <attr name="label_size_dp" format="integer" />
    </declare-styleable>


    <declare-styleable name="UserTextInput" parent="JHTGeneralWidget">

        <attr name="edit_width" format="dimension" />
        <attr name="indicator_weight" format="float" />
        <attr name="indicator_position" format="boolean"/>
        <attr name="input_type" format="enum">
            <enum name="integer_input" value="0" />
            <enum name="double_input" value="1" />
            <enum name="number_input" value="2" />
            <enum name="text_input" value="3" />
            <enum name="ip_address_input" value="4" />
            <enum name="url_input" value="5" />
            <enum name="email_input" value="6" />
            <enum name="time_input_hhmm_to_s" value="7" />
            <enum name="time_clock" value="8" />
            <enum name="date" value="9" />
        </attr>
        <attr name="min_value" format="float" />
        <attr name="max_value" format="float" />
        <attr name="input_resolution" format="float" />

        <attr name="min_chars" format="integer" />
        <attr name="max_chars" format="integer" />
        <attr name="auto_correct" format="boolean" />
        <attr name="picker_animation" format="reference" />
        <attr name="multiline" format="boolean" />
        <attr name="password" format="boolean" />
        <attr name="secondary_id" format="reference" />

        <attr name="textfilter" format="flags">
            <flag name="none" value="0"/>
            <flag name="digits" value="1"/>
            <flag name="underscores" value="2"/>
            <flag name="spaces" value="4"/>
            <flag name="allcaps" value="8"/>
        </attr>

        <attr name="error_message" format="enum">
            <enum name="none" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>

        <attr name="default_value" format="string" />


    </declare-styleable>



</resources>