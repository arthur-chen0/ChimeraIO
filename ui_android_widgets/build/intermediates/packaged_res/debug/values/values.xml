<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="button_green">#90993f</color>
    <color name="button_red">#ff0000</color>
    <color name="keypad_back_button_selected">#FF333333</color>
    <color name="keypad_button">#FFCCCCCC</color>
    <color name="keypad_button_selected">#FF777777</color>
    <color name="keypad_default_background">#99FFFFFF</color>
    <color name="matrix_yorange">#FFFCAF1A</color>
    <color name="scrollbar_thumb">#FFE6E6E6</color>
    <color name="scrollbar_track">#FF333333</color>
    <color name="selector_item_disabled_gray">#FF999999</color>
    <color name="selector_item_gray">#FFE6E6E6</color>
    <color name="selector_item_pressed_gray">#FF999999</color>
    <color name="selector_item_triangle_gray">#FFB3B3B3</color>
    <dimen name="edit_text_label_size">24dp</dimen>
    <dimen name="selector_arrow_down_margin">22dp</dimen>
    <dimen name="selector_arrow_down_margin_top">27dp</dimen>
    <dimen name="selector_arrow_margin">15dp</dimen>
    <dimen name="selector_arrow_size">16dp</dimen>
    <dimen name="selector_item_height">60dp</dimen>
    <dimen name="selector_item_help_margin">10dp</dimen>
    <dimen name="selector_item_text_end_margin">30dp</dimen>
    <dimen name="selector_item_text_margin">20dp</dimen>
    <dimen name="selector_item_text_size">20dp</dimen>
    <dimen name="selector_subitem_margin">20dp</dimen>
    <dimen name="selector_subitem_spacing">5dp</dimen>
    <dimen name="user_text_info_text">60dp</dimen>
    <integer name="selector_arrow_rotation">90</integer>
    <style name="jht_default_keyboard">
        <item name="keypad_number_key_background">@drawable/keypad_button_background</item>
        <item name="keypad_value_background">@android:color/black</item>
        <item name="keypad_back_key_background">@drawable/keypad_back_button_background</item>
        <item name="keypad_confirm_key_background">@drawable/button_key_confirm_background</item>
        <item name="keypad_confirm_key_src">@drawable/button_key_confirm</item>
        <item name="keypad_background">@color/keypad_default_background</item>
        <item name="keypad_auto_hide">0</item>
    </style>
    <style name="jht_standard_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/standard_container_border</item>
        <item name="android:padding">10dp</item>
    </style>
    <style name="jht_standard_inner_recycle_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingStart">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:scrollbarSize">5dp</item>
        <item name="android:scrollbarStyle">outsideInset</item>
        <item name="android:scrollbarTrackVertical">@color/scrollbar_track</item>
        <item name="android:scrollbarThumbVertical">@color/scrollbar_thumb</item>
        <item name="android:fadeScrollbars">false</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
    </style>
    <style name="jht_standard_inner_recycle_container_no_scroll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:padding">0dp</item>
    </style>
    <declare-styleable name="HelpButton">
        <attr format="string" name="title"/>
        <attr format="string" name="message"/>
    </declare-styleable>
    <declare-styleable name="IconTogglableButton">
        <attr format="boolean" name="firstIconShown"/>
        <attr format="boolean" name="jht_first_state"/>
        <attr format="boolean" name="jht_second_state"/>
        <attr format="boolean" name="jht_third_state"/>
    </declare-styleable>
    <declare-styleable name="JHTAutofitTextView">
	    <attr format="dimension" name="JHTAutofitTextView_minTextSize"/>
	    <attr format="boolean" name="JHTAutofitTextView_sizeToFit"/>
	    <attr format="boolean" name="JHTAutofitTextView_preferSingleLine"/>
	</declare-styleable>
    <declare-styleable name="JHTGeneralWidget">
        <attr format="enum" name="units">
            <enum name="none" value="0"/>
            <enum name="distance" value="1"/>
            <enum name="distance_small" value="2"/>
            <enum name="speed" value="3"/>
            <enum name="heart_rate" value="4"/>
            <enum name="time_seconds" value="5"/>
            <enum name="time_minutes" value="6"/>
            <enum name="time_hours" value="7"/>
            <enum name="percent" value="8"/>
            <enum name="calories" value="9"/>
            <enum name="weight" value="10"/>
            <enum name="floors" value="11"/>
            <enum name="spm" value="12"/>
            <enum name="rpm" value="13"/>
            <enum name="height" value="14"/>
            <enum name="time_years" value="15"/>
            <enum name="adc" value="16"/>
        </attr>
        <attr format="string" name="decimal_format"/>
        <attr format="enum" name="rounding_mode">
            <enum name="ceiling" value="0"/>
            <enum name="floor" value="1"/>
            <enum name="half_up" value="2"/>
            <enum name="half_down" value="3"/>
        </attr>
        <attr format="string" name="label"/>
        <attr format="boolean" name="read_only"/>
        <attr format="integer" name="label_size_dp"/>
    </declare-styleable>
    <declare-styleable name="Keypad">
        <attr format="reference" name="keypad_number_key_background"/>
        <attr format="color" name="keypad_value_background"/>
        <attr format="reference" name="keypad_back_key_background"/>
        <attr format="reference" name="keypad_confirm_key_background"/>
        <attr format="reference" name="keypad_confirm_key_src"/>
        <attr format="color" name="keypad_background"/>
        <attr format="integer" name="keypad_auto_hide"/>
    </declare-styleable>
    <declare-styleable name="PageIndicator">
        <attr format="boolean" name="horizontal"/>
        <attr format="reference" name="view_pager"/>
    </declare-styleable>
    <declare-styleable name="UserTextInput" parent="JHTGeneralWidget">

        <attr format="dimension" name="edit_width"/>
        <attr format="float" name="indicator_weight"/>
        <attr format="boolean" name="indicator_position"/>
        <attr format="enum" name="input_type">
            <enum name="integer_input" value="0"/>
            <enum name="double_input" value="1"/>
            <enum name="number_input" value="2"/>
            <enum name="text_input" value="3"/>
            <enum name="ip_address_input" value="4"/>
            <enum name="url_input" value="5"/>
            <enum name="email_input" value="6"/>
            <enum name="time_input_hhmm_to_s" value="7"/>
            <enum name="time_clock" value="8"/>
            <enum name="date" value="9"/>
        </attr>
        <attr format="float" name="min_value"/>
        <attr format="float" name="max_value"/>
        <attr format="float" name="input_resolution"/>

        <attr format="integer" name="min_chars"/>
        <attr format="integer" name="max_chars"/>
        <attr format="boolean" name="auto_correct"/>
        <attr format="reference" name="picker_animation"/>
        <attr format="boolean" name="multiline"/>
        <attr format="boolean" name="password"/>
        <attr format="reference" name="secondary_id"/>

        <attr format="flags" name="textfilter">
            <flag name="none" value="0"/>
            <flag name="digits" value="1"/>
            <flag name="underscores" value="2"/>
            <flag name="spaces" value="4"/>
            <flag name="allcaps" value="8"/>
        </attr>

        <attr format="enum" name="error_message">
            <enum name="none" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>

        <attr format="string" name="default_value"/>


    </declare-styleable>
</resources>